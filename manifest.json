{
    "name": "FullScreenSimple",
    "version": "1.0.0",
    "description": "create fullscreen button on nav bar google chrome",
    "manifest_version": 3,
    "author": "Duc Manh",
    "action": {
        "default_title": "Click to fullscreen"
    },
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
        "128": "128.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
